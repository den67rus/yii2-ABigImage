{
    "name": "den67rus/yii2-abig-image",
    "description": "This is widget wrapper of ABigImage https://github.com/makryl/ABigImage for Yii2.",
    "keywords": ["yii2", "extension", "widget", "ABigImage"],
    "homepage": "https://github.com/den67rus/yii2-abig-image",
    "type": "yii2-extension",
    "license": "Apache 2.0 License",
    "authors": [
        {
            "name": "Denis Sevostyanov",
            "email": "den67rus@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.0",
        "bower-asset/jquery-abigimage": "~2.1.0"
    },
    "autoload": {
        "psr-4": {
            "den67rus\\jqueryABigImage\\": ""
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    }
}
